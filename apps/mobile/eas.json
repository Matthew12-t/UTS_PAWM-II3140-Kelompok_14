{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "base": {
      "node": "20.18.0",
      "pnpm": "9.15.1",
      "env": {
        "EXPO_USE_METRO_WORKSPACE_ROOT": "1"
      },
      "cache": {
        "customPaths": ["../../node_modules"]
      },
      "android": {
        "preInstall": "bash eas-build-pre-install.sh"
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "preInstall": "bash eas-build-pre-install.sh"
      }
    },
    "production": {
      "extends": "base",
      "android": {
        "buildType": "apk",
        "preInstall": "bash eas-build-pre-install.sh"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
